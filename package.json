{
  "name": "education-project-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development --port 4300",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "ng-lint": "ng lint",
    "lint": "npx eslint '**/**.{js,ts}'",
    "lint-fix": "npx eslint --fix 'src/**/**.{js,ts}'",
    "style-lint": "npx stylelint 'src/**/**.{css, scss}' --cache --ignore-path -i '.stylelintignore'",
    "style-lint-fix": "npx stylelint 'src/**/**.{css, scss}' --fix",
    "prepare": "husky install",
    "prettier-check": "prettier --check 'src/**/**' --ignore-unknown",
    "prettier-write": "prettier --write 'src/**/**' --ignore-unknown"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint"
    ],
    "*.component.scss": [
      "npx stylelint"
    ],
    "**/*": "npm run prettier-check"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.8",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.8",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@taiga-ui/addon-table": "^3.59.0",
    "@taiga-ui/cdk": "^3.42.1",
    "@taiga-ui/core": "^3.42.1",
    "@taiga-ui/icons": "^3.42.1",
    "@taiga-ui/kit": "^3.42.1",
    "@taiga-ui/styles": "^3.42.1",
    "@tinkoff/ng-dompurify": "3.0.0",
    "dompurify": "2.2.9",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/dompurify": "2.2.3",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "typescript": "~4.9.4"
  }
}
